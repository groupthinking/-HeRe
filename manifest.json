{\n  "manifest_version": 2,\n  "name": "@ Shortcut Tool",\n  "version": "1.0",\n  "description": "A browser extension that provides shortcuts to various services by typing @.",\n  "permissions": [\n    "activeTab",\n    "storage"\n  ],\n  "background": {\n    "scripts": ["background.js"],\n    "persistent": false\n  },\n  "browser_action": {\n    "default_popup": "index.html",\n    "default_title": "Open @ Shortcut Tool"\n  },\n  "icons": {\n    "16": "icons/icon16.png",\n    "48": "icons/icon48.png",\n    "128": "icons/icon128.png"\n  }\n}
